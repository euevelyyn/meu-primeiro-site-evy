.unsaved-wobble { animation: 0.8s ease 0s 1 normal forwards running wobble; }

@-webkit-keyframes wobble { 
  0% { transform: none; }
  15% { transform: translate3d(-20%, 0px, 0px) rotate3d(0, 0, 1, -5deg); }
  30% { transform: translate3d(15%, 0px, 0px) rotate3d(0, 0, 1, 3deg); }
  45% { transform: translate3d(-10%, 0px, 0px) rotate3d(0, 0, 1, -3deg); }
  60% { transform: translate3d(5%, 0px, 0px) rotate3d(0, 0, 1, 2deg); }
  75% { transform: translate3d(0%, 0px, 0px) rotate3d(0, 0, 1, -1deg); }
  100% { transform: none; }
